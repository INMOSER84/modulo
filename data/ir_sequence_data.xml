<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Secuencia para Ã³rdenes de servicio -->
        <record id="sequence_service_order" model="ir.sequence">
            <field name="name">Service Order</field>
            <field name="code">service.order</field>
            <field name="prefix">SO/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="False"/>
        </record>

        <!-- Secuencia para equipos -->
        <record id="sequence_service_equipment" model="ir.sequence">
            <field name="name">Service Equipment</field>
            <field name="code">service.equipment</field>
            <field name="prefix">EQ/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="False"/>
        </record>

        <!-- Secuencia para certificados de servicio -->
        <record id="sequence_service_certificate" model="ir.sequence">
            <field name="name">Service Certificate</field>
            <field name="code">service.certificate</field>
            <field name="prefix">SC/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="False"/>
        </record>

        <!-- Secuencia para reportes de servicio -->
        <record id="sequence_service_report" model="ir.sequence">
            <field name="name">Service Report</field>
            <field name="code">service.report</field>
            <field name="prefix">SR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="False"/>
        </record>
    </data>
</odoo>
